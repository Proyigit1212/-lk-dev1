import time
import random


oyuncu1 = {"isim":"adam","sağlık":100,"güç":10}
Canavar1 = {"isim":"Gulyabani ","sağlık":150,"güç":10}
Canavar2 = {"isim":"zombi ","sağlık":50,"güç":5}
Canavar3 = {"isim":"dev kusan köpek ","sağlık":1000,"güç":15}

