                    else:
                        Canavar3["sağlık"] = Canavar3["sağlık"] - oyuncu1["güç"]
                        print(oyuncu1["isim"],Canavar3["isim"],"Saldırdın!!!")
                        print("sağlık durumu",oyuncu1["sağlık"],Canavar3["sağlık"])
                    time.sleep(1)    
