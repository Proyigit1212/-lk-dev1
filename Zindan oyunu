}
Not3 = False
Anahtar2 = False
Balta = False
Not2 = False
Not = False
oda = "Başlangıç"
anahtar = False
Med_kit = False

while True:
    print("=====================================================")
    print(oda,"odasındasınız")
    
    for giliecekoda in odalar[oda]["odalar"]:
        print(giliecekoda,"odalarına gidebilirsiniz")
    time.sleep(2)
    yeni_oda = input('Hangi odaya gitmek istersiniz?')
    if yeni_oda not in odalar[oda]["odalar"]:
        print("Böyle bir oda bulunmuyor.")
        time.sleep(2)
        continue
    if "anahtar" in odalar[oda]["öğeler"]:
        anahtar = True
        print("anahtarı buldun")

        
    
    if yeni_oda == "çıkış" and not anahtar:
        print("anahtarın yok")
        time.sleep(2)
        continue
    if yeni_oda == "çıkış":
        print("çıktın")
    if "anahtar2" in odalar[oda]["öğeler"]:
        Anahtar2 = True
        print("anahtarı buldun")
        time.sleep(2)
    if "med kit" in odalar[oda]["odalar"]:
        Med_kit = True
    if "not3" in odalar[oda]["odalar"]:
        print("bir not daha buldun bu not2nin devamı:Dev kusan bir köpek kendisi çok güçlü.Kusmuğu asitendir.Onu yenemedim.son gücümü bunu yazarak.Notlar bitti fulll notu okudunuz.")
        print("bir med kit buldun istediğn zaman med kit kulanabilirsin.yapman gereken şey medkit yazman")
    if yeni_oda == "kamera odası" and not Anahtar2:
        print("bu oda kilitli")
        continue
    if yeni_oda == "kamera odası":
        print("bu oda biraz garip.içinde birazcık huzursuzluk var")
    if "büyü asası" in odalar[oda]["odalar"]:
        print("büyü asası ultra güçlü asa sana 100 saldırı hasarı verir")
        oyuncu1["güç"] = oyuncu1["güç"] + 100
    if "harita" in odalar[oda]["öğeler"]:
        print("a")
    if "not" in odalar[oda]["öğeler"]:
        Not = True
        print("Bir not buldun şöyle diyor:Sevgili günlük.Uyandığımda ne kendimi ne de nerede olduğumu biliyordum.Olduğum yer tam olarak sarı ve huzursuz verici bir gıcırtı sesi olan ve daha çok bir labirente benzeyen bir yer. ")
    if "balta" in odalar[oda]["öğeler"]:
        print("balta buldun bu da sana totalde 25 can götürmeni sağlar ve çivilenmiş kapıları kırmanı")
        oyuncu1["güç"] = oyuncu1["güç"] + 25
        odalar[oda]["öğeler"].remove("balta")
        print("Bir not daha buldun şöyle diyor:Sevgili günlük.Burası güvenli bir yer değil tıpkı düşündüğüm gibi.Etraftaki garip yaratıklar,bubi tuzakları ve neden bilmiyorum ama dev kusan ...")
    if "gulyabani" in odalar[oda]["öğeler"]:
        print("bir gulyabani var onunla savaşman gerek!!")
        while oyuncu1["sağlık"] > 0 and Canavar1["sağlık"] > 0:
            print("sıra sende hadiii!!")
            hareket = input("hücum yap!!")
            if hareket == "hücum":
                k1 = random.randint(1,10)
                if k1 == 1:
                    Canavar1["sağlık"] = Canavar1["sağlık"] - oyuncu1["güç"]
                    print(oyuncu1["isim"],Canavar1["isim"],"saldırdın!!")
                    print("sağlık durumu",oyuncu1["sağlık"],Canavar1["sağlık"])
                else:
                    Canavar1["sağlık"] = Canavar1["sağlık"] - oyuncu1["güç"]
                    print(oyuncu1["isim"],Canavar1["isim"],"Saldırdın!!!")
                    print("sağlık durumu",oyuncu1["sağlık"],Canavar1["sağlık"])
                time.sleep(1)    
            print("sıra Gulyabani")
            hareket2 = oyuncu1["sağlık"] = oyuncu1["sağlık"] - 20
            print(oyuncu1["isim"],Canavar1["isim"],"sana saldırıyor!!")
            print("sağlık durumu",oyuncu1["sağlık"],Canavar1["sağlık"])
            
            
            if Canavar1["sağlık"] == 0:
                print("Gulyabaniyi öldürdün hadi devam")
                odalar[oda]["odalar"].remove("med kit")
                
            if oyuncu1["sağlık"] == 0:
                print("öldünnnnnn hayırrrrrrrrrrrrrrrrrrrrrr !!!Ulaşılan seviye",oda)
                break
        if oyuncu1["sağlık"] == 0:
            print("öldünnnnnn hayırrrrrrrrrrrrrrrrrrrrrr !!!Ulaşılan seviye",oda)
            break
        if yeni_oda == "CÜCE ADAM ODASI:GİRMEYİN!!!>=C":
