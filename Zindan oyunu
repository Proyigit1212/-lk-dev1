                    k1 = random.randint(1,10)
                    if k1 == 1:
                        Canavar2["sağlık"] = Canavar2["sağlık"] - oyuncu1["güç"]
                        print(oyuncu1["isim"],Canavar2["isim"],"saldırdın!!")
                        print("sağlık durumu",oyuncu1["sağlık"],Canavar2["sağlık"])
                    else:
                        Canavar2["sağlık"] = Canavar2["sağlık"] - oyuncu1["güç"]
                        print(oyuncu1["isim"],Canavar2["isim"],"Saldırdın!!!")
                        print("sağlık durumu",oyuncu1["sağlık"],Canavar2["sağlık"])
                    time.sleep(1)    
                print("sıra zombi")
                hareket2 = oyuncu1["sağlık"] = oyuncu1["sağlık"] - 20
                print(oyuncu1["isim"],Canavar2["isim"],"sana saldırıyor!!")
                print("sağlık durumu",oyuncu1["sağlık"],canavar2["sağlık"])
                
                
        if "dev kusan köpek" in odalar[oda]["öğeler"]:
            print("artık bu son kusan köpeği yok edince beta sürümünü bitirsin")
            while oyuncu1["sağlık"] > 0 and Canavar3["sağlık"] > 0:
                print("sıra sende hadiii!!")
                hareket = input("hücum yap!!")
                if hareket == "hücum":
                    k1 = random.randint(1,10)
                    if k1 == 1:
                        Canavar3["sağlık"] = Canavar3["sağlık"] - oyuncu1["güç"]
                        print(oyuncu1["isim"],Canavar3["isim"],"saldırdın!!")
